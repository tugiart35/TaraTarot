  "private": true,
  "scripts": {
    "dev": "next dev -p 3111",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "code-quality": "npm run typecheck && npm run lint && npm run format:check",
    "code-quality:fix": "npm run lint:fix && npm run format",
    "i18n:check": "node scripts/check-hardcoded-ui-strings.mjs",
    "i18n:analyze": "node scripts/i18n-key-extractor.mjs",
    "i18n:test": "npm test tests/i18n/",
    "i18n:validate": "npm run i18n:check && npm run i18n:test",
    "i18n:auto-translate": "node scripts/auto-translate-missing-keys.mjs",
