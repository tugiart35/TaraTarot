    "i18n:simple": "node scripts/simple-i18n-translator.mjs",
    "i18n:add-missing": "node scripts/add-missing-keys.mjs",
    "i18n:add-simple": "node scripts/simple-add-keys.mjs",
    "i18n:clear-cache": "node scripts/clear-i18n-cache.mjs",
    "clean:macos": "find . -name '._*' -delete && find . -name '.DS_Store' -delete",
    "test": "jest",
    "test:watch": "npm run test:continuous",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:ci && npm run test:e2e",
    "test:fast": "npm run test:quick",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:integration": "jest --testPathPattern=integration",
    "test:payment": "jest --testPathPattern=payment/__tests__",
    "test:security": "jest --testPathPattern=shopier-security.test",
    "test:webhook": "jest --testPathPattern=webhook/shopier/__tests__",
    "test:love": "node test-love-reading.js",
    "test:love:run": "npm run test:love",
    "test:quick": "node quick-test.js",
    "test:continuous": "node continuous-test.js",
    "test:real": "node real-test.js",
    "test:barbie": "npm run test:real",
    "test:user": "node real-user-test.js",
    "test:user:love": "node real-user-test.js love-spread",
    "test:user:career": "node real-user-test.js career-spread",
    "test:user:situation": "node real-user-test.js situation-analysis",
    "test:user:relationship": "node real-user-test.js relationship-analysis",
    "test:all-spreads": "npm run test:all"
